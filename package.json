{
   "name": "express-typeorm",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "scripts": {
      "build": "tsc",
      "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/app.ts",
      "start": "npm run build && NODE_ENV=production node ./dist/app.js",
      "test": "jest",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:generate": "npm run typeorm migration:generate -- -d ./src/data-source.ts",
      "migrate": "npm run typeorm migration:run -- -d ./src/data-source.ts",
      "migration:revert": "npm run typeorm migration:revert -- -d ./src/data-source.ts",
      "migration:create": "npm run typeorm migration:create --",
      "lint": "eslint --ext .ts src",
      "lint:fix": "eslint --fix --ext .ts src"
   },
   "dependencies": {
      "bcrypt": "^5.1.1",
      "class-validator-jsonschema": "^5.0.0",
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "helmet": "^7.0.0",
      "jsonwebtoken": "^9.0.2",
      "passport": "^0.6.0",
      "passport-jwt": "^4.0.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "routing-controllers": "^0.10.4",
      "routing-controllers-openapi": "^4.0.0",
      "swagger-ui-express": "^5.0.0",
      "typedi": "^0.10.0",
      "typeorm": "0.3.17"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.1",
      "@types/cors": "^2.8.15",
      "@types/express": "^4.17.20",
      "@types/jsonwebtoken": "^9.0.4",
      "@types/node": "^16.11.10",
      "@types/passport": "^1.0.14",
      "@types/passport-jwt": "^3.0.11",
      "@types/swagger-ui-express": "^4.1.5",
      "@typescript-eslint/eslint-plugin": "^6.7.2",
      "@typescript-eslint/parser": "^6.7.2",
      "eslint": "^8.50.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-prettier": "^5.0.0",
      "eslint-plugin-security": "^1.7.1",
      "husky": "^8.0.3",
      "jest": "^29.7.0",
      "prettier": "^3.0.3",
      "prettier-eslint": "^16.1.1",
      "ts-node-dev": "^2.0.0",
      "tsc-alias": "^1.8.8",
      "tsconfig-paths": "^4.2.0",
      "typescript": "5.2.2"
   }
}
